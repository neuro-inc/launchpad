{
  "$defs": {
    "ApoloFilesPath": {
      "properties": {
        "path": {
          "title": "Path",
          "type": "string",
          "x-description": "Provide the Apolo Storage path starting with `storage:` to locate your files.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Storage Path"
        }
      },
      "required": [
        "path"
      ],
      "title": "ApoloFilesPath",
      "type": "object",
      "x-description": "Specify the path within the Apolo Files application to read from or write to.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Apolo Files Path",
      "x-type": "ApoloFilesFile"
    },
    "ApoloSecret": {
      "properties": {
        "key": {
          "title": "Key",
          "type": "string"
        }
      },
      "required": [
        "key"
      ],
      "title": "ApoloSecret",
      "type": "object",
      "x-description": "Apolo Secret Configuration.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Secret",
      "x-type": "ApoloSecret"
    },
    "AppsConfig": {
      "properties": {
        "quick_start_config": {
          "anyOf": [
            {
              "$ref": "#/$defs/NoQuickStartConfig"
            },
            {
              "$ref": "#/$defs/OpenWebUIConfig"
            }
          ],
          "title": "Quick Start Config"
        }
      },
      "required": [
        "quick_start_config"
      ],
      "title": "AppsConfig",
      "type": "object",
      "x-description": "Choose some available configuration presets to jump start this Launchpad instance with some preinstalled applications",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Quickstart Presets",
      "x-type": "AppsConfig"
    },
    "CustomLLMModel": {
      "properties": {
        "model_name": {
          "title": "Model Name",
          "type": "string",
          "x-description": "The name of the custom LLM model.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Model Name"
        },
        "model_apolo_path": {
          "$ref": "#/$defs/ApoloFilesPath",
          "x-description": "Model path within the Apolo Files.",
          "x-title": "Model Path"
        },
        "server_extra_args": {
          "items": {
            "type": "string"
          },
          "title": "Server Extra Args",
          "type": "array",
          "x-description": "Additional arguments to pass to the VLLM runtime.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "VLLM Extra Arguments"
        }
      },
      "required": [
        "model_name",
        "model_apolo_path"
      ],
      "title": "CustomLLMModel",
      "type": "object",
      "x-description": "Specify a custom LLM model to be used in the Launchpad.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Custom LLM Model",
      "x-type": "CustomLLMModel"
    },
    "HuggingFaceEmbeddingsModel": {
      "properties": {
        "hf_model": {
          "$ref": "#/$defs/HuggingFaceModel",
          "x-description": "The Hugging Face model to use for text embeddings.",
          "x-meta-type": "inline"
        },
        "server_extra_args": {
          "items": {
            "type": "string"
          },
          "title": "Server Extra Args",
          "type": "array",
          "x-description": "Additional arguments to pass to the embeddings runtime.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Embeddings Extra Arguments"
        }
      },
      "required": [
        "hf_model"
      ],
      "title": "HuggingFaceEmbeddingsModel",
      "type": "object",
      "x-description": "Specify a custom embeddings model to be used in the Launchpad.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "HuggingFace Embeddings Model",
      "x-type": "HuggingFaceEmbeddingsModel"
    },
    "HuggingFaceLLMModel": {
      "properties": {
        "hf_model": {
          "$ref": "#/$defs/HuggingFaceModel",
          "x-description": "The Hugging Face model to use for the Launchpad.",
          "x-meta-type": "inline"
        },
        "server_extra_args": {
          "items": {
            "type": "string"
          },
          "title": "Server Extra Args",
          "type": "array",
          "x-description": "Additional arguments to pass to the VLLM runtime.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "VLLM Extra Arguments"
        }
      },
      "required": [
        "hf_model"
      ],
      "title": "HuggingFaceLLMModel",
      "type": "object",
      "x-description": "Specify a custom LLM model to be used in the Launchpad.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "HuggingFace LLM Model",
      "x-type": "HuggingFaceLLMModel"
    },
    "HuggingFaceModel": {
      "properties": {
        "model_hf_name": {
          "title": "Model Hf Name",
          "type": "string",
          "x-description": "The name of the Hugging Face model.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Hugging Face Model Name"
        },
        "hf_token": {
          "anyOf": [
            {
              "$ref": "#/$defs/ApoloSecret"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "x-description": "Provide a Hugging Face API token linked to an account with access to the model specified above. This token will be used to download model files from the Hugging Face Hub, including gated or private repositories where applicable.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Hugging Face Token"
        }
      },
      "required": [
        "model_hf_name"
      ],
      "title": "HuggingFaceModel",
      "type": "object",
      "x-description": "Configure the Hugging Face model. Ensure it is available on the Hugging Face Hub and provide an API token with access rights if the repository is gated.",
      "x-is-advanced-field": false,
      "x-meta-type": "integration",
      "x-title": "Hugging Face Model",
      "x-type": "HuggingFaceModel"
    },
    "LLMConfig": {
      "properties": {
        "model": {
          "anyOf": [
            {
              "$ref": "#/$defs/PreConfiguredHuggingFaceLLMModel"
            },
            {
              "$ref": "#/$defs/HuggingFaceLLMModel"
            },
            {
              "$ref": "#/$defs/CustomLLMModel"
            }
          ],
          "default": {
            "model": "meta-llama/Llama-3.1-8B-Instruct",
            "hf_token": null,
            "server_extra_args": [],
            "__type__": "PreConfiguredHuggingFaceLLMModel"
          },
          "title": "Model",
          "x-description": "Select a pre-configured LLM model for the Launchpad.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Pre-configured LLM Model"
        },
        "llm_preset": {
          "$ref": "#/$defs/Preset",
          "x-description": "Preset to use for the LLM model.",
          "x-title": "LLM Preset"
        },
        "ui_preset": {
          "$ref": "#/$defs/Preset",
          "x-description": "Preset to use for OpenWebUI.",
          "x-title": "OpenWebUI Preset"
        }
      },
      "required": [
        "llm_preset",
        "ui_preset"
      ],
      "title": "LLMConfig",
      "type": "object",
      "x-description": "Configuration for the LLM model to be used in this Launchpad.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "LLM Configuration",
      "x-type": "LLMConfig"
    },
    "LaunchpadConfig": {
      "properties": {
        "preset": {
          "$ref": "#/$defs/Preset",
          "x-description": "Preset to use for the Launchpad application.",
          "x-title": "Launchpad Preset"
        }
      },
      "required": [
        "preset"
      ],
      "title": "LaunchpadConfig",
      "type": "object",
      "x-description": "Configuration for the Launchpad application.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Launchpad Configuration",
      "x-type": "LaunchpadConfig"
    },
    "NoQuickStartConfig": {
      "properties": {
        "type": {
          "const": "no_quickstart",
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "title": "NoQuickStartConfig",
      "type": "object",
      "x-description": "Install only Launchpad. You can add new apps to Launchpad later",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "No Startup Apps",
      "x-type": "NoQuickStartConfig"
    },
    "OpenWebUIConfig": {
      "properties": {
        "llm_config": {
          "$ref": "#/$defs/LLMConfig"
        },
        "postgres_config": {
          "$ref": "#/$defs/PostgresConfig"
        },
        "embeddings_config": {
          "$ref": "#/$defs/TextEmbeddingsConfig"
        }
      },
      "required": [
        "llm_config",
        "postgres_config",
        "embeddings_config"
      ],
      "title": "OpenWebUIConfig",
      "type": "object",
      "x-description": "Install OpenWebUI and its dependencies on Launchpad startup",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "OpenWebUI Presets",
      "x-type": "OpenWebUIConfig"
    },
    "PostgresConfig": {
      "properties": {
        "preset": {
          "$ref": "#/$defs/Preset",
          "x-description": "Preset to use for the Postgres database.",
          "x-title": "Postgres Preset"
        },
        "replicas": {
          "default": 2,
          "exclusiveMinimum": 0,
          "title": "Replicas",
          "type": "integer",
          "x-description": "Number of replicas for the Postgres database.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Postgres Replicas"
        }
      },
      "required": [
        "preset"
      ],
      "title": "PostgresConfig",
      "type": "object",
      "x-description": "Configuration for the Postgres database.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Postgres Configuration",
      "x-type": "PostgresConfig"
    },
    "PreConfiguredEmbeddingsModels": {
      "enum": [
        "BAAI/bge-m3"
      ],
      "title": "PreConfiguredEmbeddingsModels",
      "type": "string"
    },
    "PreConfiguredHuggingFaceLLMModel": {
      "properties": {
        "model": {
          "$ref": "#/$defs/PreConfiguredLLMModels",
          "default": "meta-llama/Llama-3.1-8B-Instruct",
          "x-description": "Select a pre-configured LLM model from the available options.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Pre-configured Model"
        },
        "hf_token": {
          "anyOf": [
            {
              "$ref": "#/$defs/ApoloSecret"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "x-description": "Provide a Hugging Face API token for accessing gated or private models.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Hugging Face Token"
        },
        "server_extra_args": {
          "items": {
            "type": "string"
          },
          "title": "Server Extra Args",
          "type": "array",
          "x-description": "Additional arguments to pass to the VLLM runtime.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "VLLM Extra Arguments"
        }
      },
      "title": "PreConfiguredHuggingFaceLLMModel",
      "type": "object",
      "x-description": "Select a pre-configured HuggingFace LLM model with token authentication.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Pre-configured HuggingFace LLM Model",
      "x-type": "PreConfiguredHuggingFaceLLMModel"
    },
    "PreConfiguredLLMModels": {
      "enum": [
        "meta-llama/Llama-3.1-8B-Instruct",
        "unsloth/Magistral-Small-2506-GGUF"
      ],
      "title": "PreConfiguredLLMModels",
      "type": "string"
    },
    "Preset": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string",
          "x-description": "The name of the preset.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Resource Preset"
        }
      },
      "required": [
        "name"
      ],
      "title": "Preset",
      "type": "object",
      "x-description": "Select the resource preset used per service replica.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Resource Preset",
      "x-type": "Preset"
    },
    "TextEmbeddingsConfig": {
      "properties": {
        "preset": {
          "$ref": "#/$defs/Preset",
          "x-description": "Preset to use for the text embeddings service.",
          "x-title": "Text Embeddings Preset"
        },
        "model": {
          "anyOf": [
            {
              "$ref": "#/$defs/PreConfiguredEmbeddingsModels"
            },
            {
              "$ref": "#/$defs/HuggingFaceEmbeddingsModel"
            }
          ],
          "default": "BAAI/bge-m3",
          "title": "Model",
          "x-description": "The Hugging Face model to use for text embeddings.",
          "x-is-advanced-field": false,
          "x-meta-type": "inline",
          "x-title": "Embeddings Model"
        }
      },
      "required": [
        "preset"
      ],
      "title": "TextEmbeddingsConfig",
      "type": "object",
      "x-description": "Configuration for the text embeddings service.",
      "x-is-advanced-field": false,
      "x-meta-type": "inline",
      "x-title": "Text Embeddings Configuration",
      "x-type": "TextEmbeddingsConfig"
    }
  },
  "properties": {
    "launchpad_config": {
      "$ref": "#/$defs/LaunchpadConfig"
    },
    "apps_config": {
      "$ref": "#/$defs/AppsConfig"
    }
  },
  "required": [
    "launchpad_config",
    "apps_config"
  ],
  "title": "LaunchpadAppInputs",
  "type": "object"
}
